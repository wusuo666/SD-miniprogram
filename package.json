{
  "name": "ShangDi-Taro",
  "version": "1.0.0",
  "private": true,
  "description": "上地街道小e助手小程序源代码",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky",
    "new": "taro new",
    "build": "taro build --type weapp",
    "dev": "npm run build -- --watch",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,scss,sass,css,md}\" && eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix && stylelint \"src/**/*.{css,scss,sass}\" --fix"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@taroify/core": "^0.9.0",
    "@taroify/icons": "^0.9.0",
    "@tarojs/components": "4.1.9",
    "@tarojs/helper": "4.1.9",
    "@tarojs/plugin-framework-react": "4.1.9",
    "@tarojs/plugin-platform-weapp": "4.1.9",
    "@tarojs/react": "4.1.9",
    "@tarojs/runtime": "4.1.9",
    "@tarojs/shared": "4.1.9",
    "@tarojs/taro": "4.1.9",
    "protobufjs": "^6.11.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "4.1.9",
    "@tarojs/plugin-generator": "4.1.9",
    "@tarojs/taro-loader": "4.1.9",
    "@tarojs/webpack5-runner": "4.1.9",
    "@types/minimatch": "^5",
    "@types/node": "^18",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "4.1.9",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.1.9",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "webpack": "5.91.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss,sass}": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
